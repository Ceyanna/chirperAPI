<h1>Make a Chirp</h1>
<p>Find me in app/views/chirps/new.html.erb</p>
<%= form_for @chirp do |f| %>
<div>
<%= f.label :author %>
<%= f.text_field :author %>
</div>
<div>
<%= f.label :body %>
<%= f.text_area :body %>
</div>
<div>
<%= f.label :pic %>
<%= f.file_field :pic %>
</div>
<div>
<%= f.submit 'Chirp!' %>
</div>
<% end %>